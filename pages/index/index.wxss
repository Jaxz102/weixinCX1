/**index.wxss**/
.body{
    width: 100%;
    height: 100vh;
    background-position: top;
    background-size: cover;
    background-image: url('https://wallpaperaccess.com/full/1201167.jpg');
    overflow:scroll;
}
.box {  
    color: red;
    background: blue;
    height: 100px;
    width: 100px;
} 

.box-hover{
    background-color: brown;

}

image{
    background-color: red;
    display: block;
    margin: auto;
   margin-top: 20px;
}

.title{
    display: block;
    margin: auto;
    font-size: 2.5rem;
    text-align: center;
    color: #42a1f5;

}
.footer{
    margin-top: 10px;
    height: calc(50px + env(safe-area-inset-bottom));
    background-color: white;
}
navigator{
    padding: 10px 0px;
    display: block;
    text-align: center;
    background-color: yellow;
}


